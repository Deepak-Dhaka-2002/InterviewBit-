int Solution::lPalin(ListNode* A) {
    vector<int> v;
    ListNode* temp = A;
    
    while(temp != NULL){
        v.push_back(temp -> val);
        temp = temp -> next;
    }
    for(int i=0; i<v.size(); i++){
        if(v[i] != v[v.size()-1-i]){
            return 0;
        }
    }
    return 1;
}
