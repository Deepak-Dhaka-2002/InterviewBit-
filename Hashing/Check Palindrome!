 ----Check Palindrome
 
int Solution::solve(string A) {
    vector<char> c(26,0);
    for(int i=0; i<A.length(); i++){
        c[A[i]-97]++;
    }
    int a=0;
    for(int i=0; i<26; i++){
        if(c[i]%2==0)
          continue;
        else
           a++;
    }
    if(a>1)
      return 0;
    else  
     return 1;
}
