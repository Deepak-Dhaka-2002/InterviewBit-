int Solution::solve(int A, int B, int C, int D, int E) {
    int cost=INT_MAX,ans=INT_MAX,count=A,i=0;
    while(count>=0){
        if(count%D==0){
            cost=i*C+(count/D)*E;
        }
        ans=min(ans,cost);
        count-=B;
        i++;
    }
    if(ans==INT_MAX){
        return -1;
    }
    return ans;
}


